{
	"name":"svip",
	"manifest_version":2,
	"version":"2.4",
	"description":"各大网站VIP会员视频免费看，集成资源嗅探",
	"homepage_url": "https://dev.tencent.com/u/xinbingyiran/p/vip/git",
	"permissions":[
		"activeTab",
		"tabs",
		"contextMenus",
		"storage",
		"webRequest",
		"http://*/*",
		"https://*/*"
	],
	"browser_action":{
		"default_icon":"default-big.png",
		"default_title":"各大网站视频免费看"
	},
	"icons":{
		"16":"default-big.png",
		"48":"default-big.png",
		"128":"default-big.png"
	},
	"background":{
		"scripts":["background.js"]
	},
	"content_scripts":[
		{
			"matches":["<all_urls>"],
			"js":[
				"content.js"
			],
			"run_at":"document_start"
		}
	]
}